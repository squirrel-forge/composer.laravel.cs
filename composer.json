{
    "name": "squirrel-forge/lara-cs",
    "description": "Squirrel-Forge Laravel Core Support",
    "version": "0.1.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hartwell aka. siux",
            "email": "me@siux.info",
            "homepage": "https://siux.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "SquirrelForge\\Laravel\\CoreSupport\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "SquirrelForge\\Laravel\\CoreSupport\\Providers\\ServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:squirrel-forge/composer.laravel.cs.git",
            "no-api": true
        }
    ]
}
